{
  "name": "rloop",
  "version": "0.0.1",
  "private": true,
  "description": "rloop website v3",
  "main": "index.js",
  "scripts": {
    "generate_thumbs": "shjs generate_thumbs.js",
    "compile_less": "lessc src/assets/styles/main.less src/assets/styles/compiled/main.css",
    "build": "npm run compile_less && shjs compile.js",
    "_livereload": "livereload build",
    "_nodemon": "nodemon -q -e html,js,json,jpg,jpeg,png,less -i build -i node_modules -x \"npm cache clean\" -x \"npm run build\" ",
    "_httpserver": "http-server -r -c-1 build",
    "dev": "concurrently \"npm run _httpserver\" \"npm run _nodemon\""
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "http-server": "^0.9.0",
    "less": "^2.7.1",
    "livereload": "^0.4.1",
    "nodemon": "^1.9.1",
    "nunjucks": "^2.3.0",
    "shelljs": "^0.6.0"
  },
  "keywords": [
    "rloop"
  ],
  "author": "Paul Le Henaff"
}
